let streetViewService = new google.maps.StreetViewService()
          let radius = 50;

          function getStView(data, status) {
            if(status === google.maps.StreetViewStatus.OK) {
              let nearSteetViewLocation = data.location.latLng
              console.log(data)
              let heading = google.maps.geometry.spherical.computeHeading(nearSteetViewLocation, marker.position)
              largeInfoWindow.setContent(`<div> ${marker.title} </div><div id="pano"></div>`)
              let panoramaOptions = {
                position:nearSteetViewLocation,
                pov: {
                  heading: heading,
                  pitch:30
                }
              }
              let panorama = new google.maps.StreetViewPanorama(
                document.getElementById('pano'), panoramaOptions
              )
              console.log(panorama)
            }else{
              largeInfoWindow.setContent(`<div> ${marker.title} </div><div>No Street View</div>`)
            }

          }
          console.log(this.map)
          console.log(marker)
          streetViewService.getPanoramaByLocation(marker.position, radius,getStView)
          largeInfoWindow.open(this.map, marker)
        }
      })